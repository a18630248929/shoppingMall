<template name="requestNone">
  <view class="request-none">
    <image src="http://cdn.jisuapp.cn/static/webapp/images/request-none.png" mode="widthFix"></image>
    <view>{{data || '暂无数据'}}</view>
  </view>
</template>

<!-- 加载完所有数据 -->
<template name="requestAll">
  <view class="request-all">没有更多了</view>
</template>
