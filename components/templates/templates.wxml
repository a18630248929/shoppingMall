                  <view class="origin-price" wx:if="{{data.customFeature.isShowVirtualPrice}}">原价:￥{{item.original_price}}</view>
                </view>
                <view class="count-big-down" wx:if="{{!(item.status == 3 && item.end_date == '-1')}}">{{item.status !=
